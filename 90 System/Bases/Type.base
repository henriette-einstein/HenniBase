filters:
  and:
    - '!file.path.startsWith("90 System/Templates")'
    - type == link("Type")
properties:
  file.name:
    displayName: Name
  note.capital:
    displayName: Capital
  note.areas:
    displayName: Areas
  note.categories:
    displayName: Categories
views:
  - type: table
    name: All Types
    order:
      - file.name
    sort:
      - property: file.name
        direction: ASC
    columnSize:
      file.name: 643
